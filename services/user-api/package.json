{
  "name": "user-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-ci": "node dist/index.js",
    "compile": "tsc",
    "start": "ts-node src/index.ts",
    "deploy": "python deploy.py",
    "build": "python ../../scripts/test-build.py",
    "docker-shell": "python ../../scripts/test-docker-shell.py && docker run --name local-test-container -it local-test sh",
    "clean-pnpm": "python clean_pnpm.py",
    "typeorm:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/resolvedCreateAppDataSource.ts",
    "typeorm:schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./src/resolvedCreateAppDataSource.ts",
    "typeorm:migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/resolvedCreateAppDataSource.ts",
    "typeorm:migration:check": "pnpm run typeorm:migration:generate && git diff --quiet || (echo 'Pending migrations found' && exit 1)",
    "lint": "npx eslint ."
  },
  "keywords": [],
  "author": "Mars Lyceum Inc.",
  "dependencies": {
    "third-party-clients": "workspace:*",
    "@google-cloud/trace-agent": "^8.0.0",
    "@google-cloud/api-gateway": "^3.3.0",
    "@google-cloud/common": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/qs": "^6.9.15",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-serve-static-core": "^0.1.1",
    "joi": "^17.13.3",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.5",
    "user-api-client": "workspace:*",
    "group-api-client": "workspace:*",
    "common-middleware": "workspace:*",
    "yargs": "^17.7.2",
    "dotenv": "^16.4.7"
  }
}