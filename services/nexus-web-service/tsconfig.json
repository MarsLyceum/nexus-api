{
    "ts-node": {
        "transpileOnly": false,
        "files": true,
        "compilerOptions": {}
    },
    "node": {},
    "compilerOptions": {
        "target": "es2016",
        "lib": ["ESNext.AsyncIterable"],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "baseUrl": "./",
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": "./src"
    }
}
